    "d3-transition": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "g": "^2.0.1",
    "grunt": "^1.2.1",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-browserify": "^5.3.0",
