    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-uglify": "^3.4.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-newer": "^1.3.0",
    "grunt-update": "^0.2.3",
    "jit-grunt": "^0.10.0",
    "jshint-stylish": "^2.2.1",
    "shelljs": "^0.8.5",
    "time-grunt": "^1.4.0",
    "uglifyify": "^5.0.2",
    "vue": "^2.6.11"
